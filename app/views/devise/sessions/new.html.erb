<center>
  <h2>Log in</h2>
</center>
<%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { class: "form" }) do |f| %>
  <%= f.email_field :email, placeholder: "Email address",required: true, autofocus: true, autocomplete: "email", class: "input" %>
  <%= f.password_field :password,placeholder: "Password",required: true,  autocomplete: "current-password", class: "input" %>
  <% if devise_mapping.rememberable? %>
    <div class="field">
      <%= f.check_box :remember_me%>
      <%= f.label :remember_me %>
    </div>
  <% end %>
  <%= f.submit "Log in" ,class: "button"%>
  <br>
  <%=  link_to "You Dont't Have Account ?", new_userdevise_registration_path %><br>
  <%=  link_to "Forgot Passowrd ?", new_userdevise_password_path %>
<% end %>
